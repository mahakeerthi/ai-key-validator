{
  "name": "ai-key-validator",
  "version": "0.1.0",
  "description": "A secure npm package for validating API keys from major AI providers (OpenAI, Claude, Gemini)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai-key-validator": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && npm test",
    "clean": "rm -rf dist",
    "prepack": "npm run clean && npm run build"
  },
  "keywords": [
    "ai",
    "api-key",
    "validation",
    "openai",
    "claude",
    "gemini",
    "anthropic",
    "google",
    "cli",
    "typescript",
    "security"
  ],
  "author": "mahakeerthi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahakeerthi/ai-key-validator.git"
  },
  "bugs": {
    "url": "https://github.com/mahakeerthi/ai-key-validator/issues"
  },
  "homepage": "https://github.com/mahakeerthi/ai-key-validator#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
